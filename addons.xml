<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<addons>
  <addon id="repository.vietmediaf" name="VietMediaF Repository" version="0.0.1" provider-name="VietMediaF">
    <extension point="xbmc.addon.repository">
      <info compressed="false">https://bioidaika.github.io/addons.xml</info>
      <checksum>https://bioidaika.github.io/addons.xml.md5</checksum>
      <datadir zip="true">https://bioidaika.github.io/</datadir>
    </extension>
    <extension point="xbmc.addon.metadata">
      <summary>VietMediaF Repository</summary>
      <description>Repository containing KODI add-ons</description>
      <platform>all</platform>
    </extension>
  </addon>

  <addon id="plugin.video.hdvn" name="HDVN" version="1.2.5" provider-name="HDVN">
    <requires>
      <import addon="xbmc.python" version="3.0.0"/>
    </requires>
    <extension point="xbmc.python.pluginsource" library="default.py">
      <provides>video</provides>
    </extension>
    <extension point="xbmc.addon.metadata">
      <summary>HDVN Video Add-on</summary>
      <description>HDVN Video Add-on for KODI</description>
      <platform>all</platform>
    </extension>
  </addon>

  <addon id="repository.jellyfin.kodi" name="Jellyfin Repository" version="1.0.0" provider-name="Jellyfin">
    <extension point="xbmc.addon.repository">
      <info compressed="false">https://bioidaika.github.io/addons.xml</info>
      <checksum>https://bioidaika.github.io/addons.xml.md5</checksum>
      <datadir zip="true">https://bioidaika.github.io/</datadir>
    </extension>
    <extension point="xbmc.addon.metadata">
      <summary>Jellyfin Repository</summary>
      <description>Jellyfin Repository for KODI</description>
      <platform>all</platform>
    </extension>
  </addon>
</addons>
